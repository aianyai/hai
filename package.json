{
  "name": "@aiany/hai",
  "version": "0.8.2",
  "description": "AI in your terminal. Run commands or ask questions - just hai it. Supports OpenAI, Claude, Gemini, DeepSeek & more.",
  "type": "module",
  "bin": {
    "hai": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "ai",
    "cli",
    "terminal",
    "command-line",
    "agent",
    "openai",
    "anthropic",
    "claude",
    "gemini",
    "gpt",
    "llm",
    "chatgpt",
    "copilot",
    "assistant",
    "shell",
    "unix",
    "pipe",
    "automation",
    "term",
    "powershell",
    "cmd"
  ],
  "author": "aiany",
  "license": "GPL-3.0",
  "homepage": "https://github.com/aianyai/hai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aianyai/hai.git"
  },
  "bugs": {
    "url": "https://github.com/aianyai/hai/issues"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.0",
    "@ai-sdk/google": "^3.0.0",
    "@ai-sdk/openai": "^3.0.0",
    "@ai-sdk/openai-compatible": "^2.0.0",
    "ai": "^6.0.0",
    "chalk": "^5.4.0",
    "commander": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.52.0"
  },
  "engines": {
    "node": ">=18"
  }
}
